{
  "public": true,
  "version": 2,
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.php" },
    { "src": "/assets/(.*)", "dest": "/public/assets/$1" },
    { "src": "/dist/(.*)", "dest": "/dist/$1" },
    { "src": "/api/(.*)", "dest": "/api/$1" }
  ],
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.3"
    }
  }
}
