{
  "version": 2,
  "public": true,
  "outputDirectory": "api/public/dist",
  "rewrites": [
    { "source": "/api/public/(.*)", "destination": "/api/public/$1" },
    { "source": "/(.*)", "destination": "/api/index.php" }
  ],
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.3"
    }
  }
}
